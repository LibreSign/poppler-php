{
  "name": "giliomeejg/poppler-php",
  "description": "Complete, Comprehensive and Flexible PHP wrapper for Poppler-utils",
  "keywords": [
    "Poppler-utils",
    "Poppler-PHP",
    "PDF",
    "PDF to HTML",
    "PDF to SVG",
    "PDF to JPG",
    "PDF to PNG",
    "PDF Convert",
    "PHP-PDF"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Gerrit Giliomee",
      "email": "gerrit@nudge.tech"
    }
  ],
  "require": {
    "php": ">=7.3",
    "illuminate/config": "^7.9",
    "illuminate/support": "^7.9"
  },
  "require-dev": {
    "phpunit/phpunit": "9.1.*"
  },
  "autoload": {
    "psr-4": {
      "NcJoes\\PopplerPhp\\": "src/PopplerPhp"
    }
  },
  "minimum-stability": "stable"
}
